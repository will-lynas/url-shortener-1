# Port on which the server will run (default: 8080)
PORT=8080

# Path to the SQLite database file (default: database/database.sqlite3)
DB_PATH=database/database.sqlite3

# Google Safe Browsing API key (optional)
# If not provided, the safe browsing feature will be disabled
# SAFE_BROWSING_API_KEY=your_google_safe_browsing_api_key

# Session secret key for encrypting session data (optional)
# If not provided, a random string will be used
# It is recommended to set this to a secure random string in production
# SESSION_SECRET_KEY=change_this_to_a_secure_random_string