# Port on which the server will run (default: 8080)
PORT=8080

# Path to the SQLite database file (default: database/database.sqlite3)
DB_PATH=database/database.sqlite3

# Google Safe Browsing API key
SAFE_BROWSING_API_KEY=your_google_safe_browsing_api_key
SAFE_BROWSING_DB_PATH=database/safebrowsing_db

# JWT secret key
JWT_SECRET_KEY=change_this_to_a_secure_random_string

# TLS certificate and key paths
TLS_CERT_PATH=cert.pem
TLS_KEY_PATH=key.pem
